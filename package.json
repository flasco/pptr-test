{
  "name": "pptr-test",
  "version": "1.2.2",
  "description": "i love learn",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "node index.js",
    "dev": "SWCRC=true node -r @swc-node/register ./src/index.ts",
    "build": "swc src -d ./dist"
  },
  "dependencies": {
    "appdata-path": "^1.0.0",
    "axios": "^1.3.5",
    "dayjs": "^1.11.7",
    "node-notifier": "^10.0.1",
    "open": "^7.3.0",
    "puppeteer-core": "^19.9.1",
    "tslib": "^2.5.0"
  },
  "devDependencies": {
    "@swc-node/register": "^1.6.4",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.51",
    "@types/node": "^12.0.12",
    "@types/node-notifier": "^8.0.2",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "~7.7.0",
    "eslint-config-prettier": "~6.11.0",
    "husky": "^3.0.0",
    "jest": "^24.8.0",
    "lint-staged": "8.x",
    "typescript": "^5.0.4"
  },
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$",
      "<rootDir>/node_modules/"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{js,ts}": [
        "eslint",
        "git add"
      ]
    },
    "ignore": [
      "**/dist/**/*",
      ".eslint*.*"
    ]
  }
}
