{
  "name": "pptr-test",
  "version": "1.2.2",
  "description": "i love learn",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "node index.js",
    "dev": "tsc --watch",
    "build": "rm -rf dist && tsc --project tsconfig.prod.json"
  },
  "dependencies": {
    "@flasco/logger": "0.0.3",
    "appdata-path": "^1.0.0",
    "axios": "^0.19.0",
    "date-fns": "^1.30.1",
    "node-notifier": "^8.0.1",
    "open": "^7.3.0",
    "puppeteer-core": "^5.3.1",
    "tslib": "^2.0.3"
  },
  "devDependencies": {
    "@types/node": "^12.0.12",
    "@types/node-notifier": "^5.4.0",
    "@types/puppeteer-core": "^1.9.0",
    "@typescript-eslint/eslint-plugin": "~3.9.1",
    "@typescript-eslint/parser": "~3.9.1",
    "eslint": "~7.7.0",
    "eslint-config-prettier": "~6.11.0",
    "husky": "^3.0.0",
    "jest": "^24.8.0",
    "lint-staged": "8.x",
    "typescript": "^4.0.3"
  },
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$",
      "<rootDir>/node_modules/"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{js,ts}": [
        "eslint",
        "git add"
      ]
    },
    "ignore": [
      "**/dist/**/*",
      ".eslint*.*"
    ]
  }
}
